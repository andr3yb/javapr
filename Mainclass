public class Main {
    public static void main(String[] args) {
        Rect rect = new Rect();
        Rect rect2 = new Rect();
        rect.width = 1;
        rect.height = 2;
        System.out.println(rect.getArea());
        rect2.width = 5;
        rect2.height = 1;
        System.out.println(rect2.getPerimeter());
    }
}
